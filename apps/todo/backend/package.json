{
  "name": "@incubator/todo-backend",
  "sideEffects": false,
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/esm/index.js"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm compile:cjs && pnpm compile:esm",
    "clean": "shx rm -rf dist",
    "compile:cjs": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "compile:esm": "tsc -p tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/cjs/index.js"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.2",
    "@hono/zod-validator": "^0.2.0",
    "@incubator/shared": "workspace:*",
    "@prisma/client": "5.11.0",
    "argon2": "^0.40.1",
    "dotenv": "^16.4.5",
    "hono": "^4.1.1"
  },
  "devDependencies": {
    "@incubator/typescript-config": "workspace:*",
    "@types/node": "^20.11.28",
    "prisma": "^5.11.0",
    "shx": "^0.3.4",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  }
}
