{
  "name": "@incubator/shared",
  "version": "0.0.1",
  "private": true,
  "description": "Shared interfaces and functions",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm compile",
    "clean": "shx rm -rf dist",
    "compile": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@incubator/eslint-config": "workspace:*",
    "@incubator/typescript-config": "workspace:*",
    "@types/node": "^20.11.28",
    "eslint": "^8.57.0",
    "shx": "^0.3.4",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.2"
  }
}
