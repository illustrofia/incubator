{
  "name": "@incubator/shared",
  "version": "0.0.1",
  "private": true,
  "description": "Shared interfaces and functions",
  "sideEffects": false,
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/esm/index.js"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm compile:cjs && pnpm compile:esm",
    "clean": "shx rm -rf dist",
    "compile:cjs": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "compile:esm": "tsc -p tsconfig.esm.json && tsc-alias -p tsconfig.esm.json"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@incubator/eslint-config": "workspace:*",
    "@incubator/typescript-config": "workspace:*",
    "@types/node": "^20.11.28",
    "eslint": "^8.57.0",
    "shx": "^0.3.4",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.2"
  }
}
